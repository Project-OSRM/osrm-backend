{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "declaration": false,
    "outDir": "./features/support",
    "rootDir": "./features/support",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["features/support/**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
